{
  "name": "dagre-d3-renderer",
  "version": "0.5.8",
  "description": "A D3-based renderer for Dagre",
  "keywords": [
    "graph",
    "dagre",
    "graphlib",
    "graphlibrary",
    "renderer",
    "d3"
  ],
  "main": "dist/dagre-d3.core.js",
  "scripts": {
    "lint": "standard",
    "karma": "node -r babel-register ./node_modules/.bin/karma start --single-run",
    "test": "yarn lint && yarn karma && node test/demo-test.js",
    "upgrade": "yarn-upgrade-all",
    "build": "webpack --progress --colors",
    "build:watch": "yarn build --watch",
    "release": "yarn build -p",
    "prepush": "yarn test",
    "prepublishOnly": "yarn release"
  },
  "dependencies": {
    "dagre-layout": "^0.8.8",
    "lodash": "^4.17.5"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "husky": "^0.14.3",
    "karma": "^6.3.14",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-safari-launcher": "^1.0.0",
    "mocha": "^5.0.1",
    "puppeteer": "^1.1.1",
    "standard": "^11.0.0",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.10",
    "webpack-node-externals": "^1.6.0",
    "yarn-upgrade-all": "^0.3.0"
  },
  "peerDependencies": {
    "d3": "^4.13.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tylingsoft/dagre-d3-renderer.git"
  },
  "license": "MIT",
  "standard": {
    "ignore": [
      "dist/**/*.js"
    ]
  }
}
